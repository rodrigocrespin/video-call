{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "ExchangeRates": "Debug",
        "MassTransit": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "standardErrorFromLevel": "Warning"
        }
      }
    ],
    "Properties": {
      "Application": "Video Call API"
    }
  },
  "AllowedHosts": "*"
}
